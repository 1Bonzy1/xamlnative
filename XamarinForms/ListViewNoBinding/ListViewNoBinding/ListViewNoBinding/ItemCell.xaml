    <Grid.ColumnDefinitions>
      <ColumnDefinition Width="50"/>
      <ColumnDefinition Width="*"/>
    </Grid.ColumnDefinitions>
    <Label x:Name="idLabel" />
    <Label x:Name="textLabel" Grid.Column="1" />
  </Grid>
</ViewCell>